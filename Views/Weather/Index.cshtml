@model List<WeatherDataView>
<style>
    table {
        border-collapse: collapse;
        width: 100%;
        margin-top: 20px;
    }

    th, td {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    th {
        background-color: #f2f2f2;
    }

    tr:hover {
        background-color: #f5f5f5;
    }
</style>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Temperature</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var weatherData in Model)
        {
            <tr>
                <td>@weatherData.Date</td>
                <td>@weatherData.Temperature</td>
            </tr>
        }
    </tbody>
</table>